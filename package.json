{
  "name": "scraping-server-italy-romania",
  "version": "1.0.0",
  "description": "Web scraping server for Italian and Romanian software companies",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "serve:directory": "node serve-directory.js",
    "test": "npm run test:proxies && npm run test:scrapers",
    "test:all": "npm run test:proxies && npm run test:scrapers",
    "test:proxies": "node scripts/testing/test-proxies.js",
    "test:scrapers": "npm run scrape:italy && npm run scrape:romania",
    "test:italy": "node scripts/testing/test-italy-scrapers.js",
    "test:romania": "node scripts/testing/test-romania-scrapers.js",
    "scrape:italy": "node scripts/testing/test-italy-scrapers.js",
    "scrape:romania": "node scripts/testing/test-romania-scrapers.js",
    "scrape:test": "node scripts/testing/test-test-scraper.js",
    "scrape:new": "node scripts/testing/test-new-scrapers.js",
    "download:pages": "node scripts/testing/download-pages.js",
    "test:real": "node scripts/testing/test-real-scraper.js",
    "test:all": "node scripts/testing/test-all-real-scrapers.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "db:migrate": "node scripts/setup/init-database.js",
    "worker": "node src/workers/scrapeWorker.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "axios": "^1.12.2",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dompurify": "^3.3.0",
    "dotenv": "^16.6.1",
    "email-validator": "^2.0.4",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.0",
    "fast-csv": "^5.0.5",
    "helmet": "^7.2.0",
    "i18n-iso-countries": "^7.14.0",
    "iban": "^0.0.14",
    "ioredis": "^5.8.2",
    "iso-3166-1": "^2.1.1",
    "joi": "^17.13.3",
    "jsdom": "^23.2.0",
    "json2csv": "^5.0.7",
    "libphonenumber-js": "^1.12.24",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "morgan": "^1.10.1",
    "natural": "^6.12.0",
    "node-geocoder": "^4.4.1",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^4.7.1",
    "string-similarity": "^4.0.4",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.18.3",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
